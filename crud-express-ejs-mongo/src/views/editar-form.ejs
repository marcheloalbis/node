<% include cabecera %> <div class="container">
    <div class="row">
        <div class="col-md-5 offset-md-3">
            <div class="card">
                <div class="card-body">
                    <form action="/editar/<%= alumno.ci %>" method="post">
                        <div class="form-group"> <input class="form-control" type="text" name="ci"
                                value="<%= alumno.ci %>" placeholder="CI" readonly> </div>
                        <div class="form-group"> <input class="form-control" type="text" name="nombres"
                                value="<%= alumno.nombres %>" placeholder="Nombres"> </div>
                        <div class="form-group"> <input class="form-control" type="text" name="apellidos"
                                value="<%= alumno.apellidos %>" placeholder="Apellidos"> </div>
                        <div class="d-flex"> <button class="btn btn-success" type="submit"> Actualizar </button> <a
                                href="/" class="btn btn-info ml-2">Cancelar</a> </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div> <% include pie %>